<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd"
             version="2.2">
    <persistence-unit name="projetsgbd" transaction-type="JTA">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <jta-data-source>java:comp/env/jdbc/LocationVehicules</jta-data-source>
        <class>com.entity.Adresse</class>
        <class>com.entity.Autorise</class>
        <class>com.entity.Choisir</class>
        <class>com.entity.Contient</class>
        <class>com.entity.Contrat</class>
        <class>com.entity.Couleur</class>
        <class>com.entity.Entrepot</class>
        <class>com.entity.Facture</class>
        <class>com.entity.Marque</class>
        <class>com.entity.Modele</class>
        <class>com.entity.OptionSupplementaire</class>
        <class>com.entity.OptionVehicule</class>
        <class>com.entity.Paiement</class>
        <class>com.entity.Pays</class>
        <class>com.entity.Permission</class>
        <class>com.entity.Reservation</class>
        <class>com.entity.Role</class>
        <class>com.entity.Telephone</class>
        <class>com.entity.Utilisateur</class>
        <class>com.entity.Vehicule</class>
        <class>com.entity.Ville</class>

        <properties>
            <property name="eclipselink.logging.level" value="WARNING" />
        </properties>
    </persistence-unit>
</persistence>
